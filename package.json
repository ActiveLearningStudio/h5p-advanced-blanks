{
  "name": "h5p-advanced-blanks",
  "version": "0.1.0",
  "description": "Fill the blanks exercise with individual feedback",
  "private": true,
  "scripts": {
    "test": "ava",
    "build": "cross-env NODE_ENV='development' webpack",
    "build:prod": "cross-env NODE_ENV='production' webpack -p",
    "watch": "webpack --watch",
    "dist": "cp library.json dist && cp semantics.json dist",
    "deploy": "rsync -vaz --delete dist/* ../remote/h5p-advanced-blanks",
    "build+deploy": "npm run build && npm run deploy",
    "bdd": "npm run build && npm run dist && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sr258/h5p-advanced-blanks.git"
  },
  "keywords": [
    "h5p",
    "library"
  ],
  "author": "Joubel",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^1.10.33",
    "@types/ractive": "^0.7.27",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.26.2",
    "diff": "^3.3.0",
    "html-loader": "^0.5.1",
    "postcss-loader": "^2.0.6",
    "style-loader": "^0.13.2",
    "ts-lint": "^4.5.1",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "webpack": "^2.2.1"
  }
}
